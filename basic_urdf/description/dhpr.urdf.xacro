<?xml version="1.0"?>
<robot name="dhpr" xmlns:xacro="http://ros.org/wiki/xacro">
  <!-- Includes -->
  <xacro:include filename="$(find basic_urdf)/description/dhpr.gazebo" />
  <xacro:include filename="$(find basic_urdf)/description/materials.xacro" />
  <!-- Macros -->
  <xacro:macro name="solid_cuboid_inertia" params="m l w h">
    <inertia ixx="${(m*(w*w+h*h))/12}" ixy="0" ixz="0" iyy="${(m*(l*l+h*h))/12}" iyz="0"
      izz="${(m*(l*l+w*w))/12}" />
  </xacro:macro>
  <!-- BaseFootprint -->
  <link name="base_footprint"></link>
  <!-- Joint BaseFootprint -> BaseLink -->
  <joint name="base_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="base_footprint" />
    <child link="base_link" />
  </joint>
  <!-- BaseLink -->
  <link name='base_link'>
    <inertial>
      <mass value="15.0" />
      <origin xyz="0 0 0.1" rpy=" 0 0 0" />
      <xacro:solid_cuboid_inertia m="15.0" l="0.8" w="0.6" h="0.2" />
    </inertial>
    <collision name='collision'>
      <origin xyz="0 0 0.1" rpy=" 0 0 0" />
      <geometry>
        <box size="0.8 0.6 0.2" />
      </geometry>
    </collision>
    <visual name='base_link_visual'>
      <material name="grey" />
      <origin xyz="0 0 0.1" rpy=" 0 0 0" />
      <geometry>
        <box size="0.8 0.6 0.2" />
      </geometry>
    </visual>
  </link>
  <!-- Joint BaseLink -> FrontLeftWheel -->
  <joint type="continuous" name="front_left_wheel_joint">
    <origin xyz="0.25 0.38 0.05" rpy="0 0 0" />
    <child link="front_left_wheel" />
    <parent link="base_link" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="100" velocity="10" />
    <dynamics damping="1.0" friction="1.0" />
  </joint>
  <!-- FrontLeftWheel -->
  <link name='front_left_wheel'>
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <inertia ixx="0.014" ixy="0" ixz="0" iyy="0.014" iyz="0" izz="0.025" />
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.15" />
      </geometry>
    </collision>
    <visual name='front_left_wheel_visual'>
      <material name="black" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.15" />
      </geometry>
    </visual>
  </link>
  <!-- Joint BaseLink -> FrontRightWheel -->
  <joint type="continuous" name="front_right_wheel_joint">
    <origin xyz="0.25 -0.38 0.05" rpy="0 0 0" />
    <child link="front_right_wheel" />
    <parent link="base_link" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="100" velocity="10" />
    <dynamics damping="1.0" friction="1.0" />
  </joint>
  <!-- FrontRightWheel -->
  <link name='front_right_wheel'>
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <inertia ixx="0.014" ixy="0" ixz="0" iyy="0.014" iyz="0" izz="0.025" />
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.15" />
      </geometry>
    </collision>
    <visual name='front_right_wheel_visual'>
      <material name="black" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.15" />
      </geometry>
    </visual>
  </link>
  <!-- Joint BaseLink -> LeftWheel -->
  <joint type="continuous" name="left_wheel_joint">
    <origin xyz="-0.25 0.38 0.05" rpy="0 0 0" />
    <child link="left_wheel" />
    <parent link="base_link" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="100" velocity="10" />
    <dynamics damping="1.0" friction="1.0" />
  </joint>
  <!-- LeftWheel -->
  <link name='left_wheel'>
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <inertia ixx="0.014" ixy="0" ixz="0" iyy="0.014" iyz="0" izz="0.025" />
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.15" />
      </geometry>
    </collision>
    <visual name='left_wheel_visual'>
      <material name="black" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.15" />
      </geometry>
    </visual>
  </link>
  <!-- Joint BaseLink -> RightWheel -->
  <joint type="continuous" name="right_wheel_joint">
    <origin xyz="-0.25 -0.38 0.05" rpy="0 0 0" />
    <child link="right_wheel" />
    <parent link="base_link" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="100" velocity="10" />
    <dynamics damping="1.0" friction="1.0" />
  </joint>
  <!-- RightWheel -->
  <link name='right_wheel'>
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <inertia ixx="0.014" ixy="0" ixz="0" iyy="0.014" iyz="0" izz="0.025" />
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.15" />
      </geometry>
    </collision>
    <visual name='right_wheel_visual'>
      <material name="black" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.15" />
      </geometry>
    </visual>
  </link>
  <!-- Gazebo -->
  <gazebo reference="front_left_wheel">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <minDepth>0.0001</minDepth>
    <maxVel>1.0</maxVel>
  </gazebo>
  <gazebo reference="front_right_wheel">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <minDepth>0.0001</minDepth>
    <maxVel>1.0</maxVel>
  </gazebo>
  <gazebo reference="left_wheel">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <minDepth>0.0001</minDepth>
    <maxVel>1.0</maxVel>
  </gazebo>
  <gazebo reference="right_wheel">
    <mu1>0.2</mu1>
    <mu2>0.2</mu2>
    <kp>1000000.0</kp>
    <kd>100.0</kd>
    <minDepth>0.0001</minDepth>
    <maxVel>1.0</maxVel>
  </gazebo>
  <gazebo reference="base_link">
    <mu1>0.000002</mu1>
    <mu2>0.000002</mu2>
  </gazebo>
</robot>