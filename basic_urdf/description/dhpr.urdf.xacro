<?xml version="1.0"?>
<robot name="dhpr" xmlns:xacro="http://ros.org/wiki/xacro">
  <!-- Includes -->
  <xacro:include filename="$(find basic_urdf)/description/dhpr.gazebo" />
  <xacro:include filename="$(find basic_urdf)/description/materials.xacro" />
  <!-- Macros -->
  <xacro:macro name="solid_cuboid_inertia" params="m l w h">
    <inertia ixx="${(m*(w*w+h*h))/12}" ixy="0" ixz="0" iyy="${(m*(l*l+h*h))/12}" iyz="0"
      izz="${(m*(l*l+w*w))/12}" />
  </xacro:macro>
  <!-- BaseFootprint -->
  <link name="base_footprint"></link>
  <!-- Joint BaseFootprint -> BaseLink -->
  <joint name="base_footprint_joint" type="fixed">
    <origin xyz="0 0 0.1" rpy="0 0 0" />
    <parent link="base_footprint" />
    <child link="base_link" />
  </joint>
  <!-- BaseLink -->
  <link name='base_link'>
    <pose>0 0 0.1 0 0 0</pose>
    <inertial>
      <mass value="15.0" />
      <origin xyz="0 0 0.1" rpy=" 0 0 0" />
      <xacro:solid_cuboid_inertia m="15.0" l="0.8" w="0.7" h="0.2" />
    </inertial>
    <collision name='collision'>
      <origin xyz="0 0 0.1" rpy=" 0 0 0" />
      <geometry>
        <box size="0.8 0.7 0.2" />
      </geometry>
    </collision>
    <visual name='base_link_visual'>
      <material name="grey" />
      <origin xyz="0 0 0.1" rpy=" 0 0 0" />
      <geometry>
        <box size="0.8 0.7 0.2" />
      </geometry>
    </visual>
  </link>
  <!-- Joint BaseLink -> FrontLeftWheel -->
  <joint type="continuous" name="front_left_wheel_joint">
    <origin xyz="0.25 0.38 0.05" rpy="0 0 0" />
    <child link="front_left_wheel" />
    <parent link="base_link" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="100" velocity="10"/>
    <dynamics damping="1.0" friction="1.0" />
  </joint>
  <!-- FrontLeftWheel -->
  <link name='front_left_wheel'>
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <inertia ixx="0.014" ixy="0" ixz="0" iyy="0.014" iyz="0" izz="0.025" />
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.10" />
      </geometry>
    </collision>
    <visual name='front_left_wheel_visual'>
      <material name="orange" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.10" />
      </geometry>
    </visual>
  </link>
  <!-- Joint BaseLink -> FrontRightWheel -->
  <joint type="continuous" name="front_right_wheel_joint">
    <origin xyz="0.25 -0.38 0.05" rpy="0 0 0" />
    <child link="front_right_wheel" />
    <parent link="base_link" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="100" velocity="10"/>
    <dynamics damping="1.0" friction="1.0" />
  </joint>
  <!-- FrontRightWheel -->
  <link name='front_right_wheel'>
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <inertia ixx="0.014" ixy="0" ixz="0" iyy="0.014" iyz="0" izz="0.025" />
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.10" />
      </geometry>
    </collision>
    <visual name='front_right_wheel_visual'>
      <material name="orange" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.10" />
      </geometry>
    </visual>
  </link>
  <!-- Joint BaseLink -> RearLeftWheel -->
  <joint type="continuous" name="rear_left_wheel_joint">
    <origin xyz="-0.25 0.38 0.05" rpy="0 0 0" />
    <child link="rear_left_wheel" />
    <parent link="base_link" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="100" velocity="10"/>
    <dynamics damping="1.0" friction="1.0" />
  </joint>
  <!-- LeftWheel -->
  <link name='rear_left_wheel'>
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <inertia ixx="0.014" ixy="0" ixz="0" iyy="0.014" iyz="0" izz="0.025" />
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.10" />
      </geometry>
    </collision>
    <visual name='rear_left_wheel_visual'>
      <material name="orange" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.10" />
      </geometry>
    </visual>
  </link>
  <!-- Joint BaseLink -> RearRightWheel -->
  <joint type="continuous" name="rear_right_wheel_joint">
    <origin xyz="-0.25 -0.38 0.05" rpy="0 0 0" />
    <child link="rear_right_wheel" />
    <parent link="base_link" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="100" velocity="10"/>
    <dynamics damping="1.0" friction="1.0" />
  </joint>
  <!-- RightWheel -->
  <link name='rear_right_wheel'>
    <inertial>
      <mass value="5.0" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <inertia ixx="0.014" ixy="0" ixz="0" iyy="0.014" iyz="0" izz="0.025" />
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.10" />
      </geometry>
    </collision>
    <visual name='rear_right_wheel_visual'>
      <material name="orange" />
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder radius="0.15" length="0.10" />
      </geometry>
    </visual>
  </link>
  <!-- Joint BaseLink -> Lidar -->
  <joint type="fixed" name="lidar_joint">
    <origin xyz="0 0 0.20" rpy="0 0 0" />
    <child link="lidar_link" />
    <parent link="base_link" />
  </joint>
  <!-- Lidar -->
  <link name='lidar_link'>
    <inertial>
      <mass value="0.3" />
      <origin xyz="0 0 0" rpy="0 0 0" />
      <inertia ixx="0.0013125" ixy="0" ixz="0" iyy="0.0013125" iyz="0" izz="0.0015" />
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.15 0.15 0.15" />
      </geometry>
    </collision>
    <visual name='lidar_link_visual'>
      <material name="blue" />
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.13" length="0.10" />
      </geometry>
    </visual>
  </link>
</robot>
